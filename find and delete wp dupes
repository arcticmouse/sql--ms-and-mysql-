create temporary table tmpTable (id int)

insert  tmpTable
        (id)
select  id
from    wp_16_posts yt
where   exists
        (
        select  *
        from    wp_16_posts yt2
        where   yt2.post_type = 'story'
        and yt2.post_title = yt.post_title
        and yt2.post_date = yt.post_date
        and yt2.ID > yt.ID
        )
        
delete temporary table tmpTable
